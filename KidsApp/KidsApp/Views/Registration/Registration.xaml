<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KidsApp.Views.Registration.Registration"
             Title="Atras         Registro">

    
    <Label Text="Nombre:" VerticalTextAlignment="Start" HorizontalTextAlignment="Start"></Label>
    <Entry Placeholder="Escriba su nombre" HorizontalTextAlignment="Start"></Entry>
    <Label Text="Apellido:" VerticalTextAlignment="Start" HorizontalTextAlignment="Start"></Label>
    <Entry Placeholder="Escriba su apellido" IsPassword="True" HorizontalTextAlignment="Center"></Entry>
    <Label Text="Edad:" VerticalTextAlignment="Start" HorizontalTextAlignment="Start"></Label>
    <Entry Placeholder="Escriba su edad" IsPassword="True" HorizontalTextAlignment="Center"></Entry>
    

</ContentPage>